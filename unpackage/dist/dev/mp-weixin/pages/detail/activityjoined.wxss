.page-container.data-v-beed2d58 {
  height: 100vh;
  background: #f5f5f5;
}
.content-container.data-v-beed2d58 {
  height: 100vh;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}


/* 自定义导航栏样式 */
.custom-navbar.data-v-beed2d58 {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 1000;
  background: linear-gradient(135deg, #ff4757, #ff3742);
  box-shadow: 0 2rpx 10rpx rgba(255, 71, 87, 0.3);
}
.status-bar.data-v-beed2d58 {
  width: 100%;
  background: transparent;
}
.navbar-content.data-v-beed2d58 {
  height: 88rpx;
  display: flex;
  align-items: center;
  padding: 0 32rpx;
  position: relative;
}
.navbar-left.data-v-beed2d58 {
  width: 88rpx;
  height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
  transition: background-color 0.2s ease;
}
.navbar-left.data-v-beed2d58:active {
  background-color: rgba(255, 255, 255, 0.1);
}
.back-icon.data-v-beed2d58 {
  font-size: 40rpx;
  color: white;
  font-weight: bold;
  line-height: 1;
}
.navbar-center.data-v-beed2d58 {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  display: flex;
  align-items: center;
  justify-content: center;
}
.navbar-title.data-v-beed2d58 {
  font-size: 36rpx;
  font-weight: bold;
  color: white;
  text-shadow: 0 2rpx 4rpx rgba(0, 0, 0, 0.1);
}
.navbar-right.data-v-beed2d58 {
  width: 88rpx;
  height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}


/* Tab栏样式 */
.tab-bar.data-v-beed2d58 {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 999;
  background: white;
  box-shadow: 0 2rpx 8rpx rgba(0, 0, 0, 0.1);
}
.tab-bar-placeholder.data-v-beed2d58 {
  width: 100%;
  background: transparent;
}
.tab-container.data-v-beed2d58 {
  display: flex;
  height: 100rpx;
  background: white;
  border-bottom: 1rpx solid #f0f0f0;
}
.tab-item.data-v-beed2d58 {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: relative;
  transition: all 0.3s ease;
}
.tab-item.data-v-beed2d58:active {
  background-color: rgba(255, 71, 87, 0.05);
}
.tab-text.data-v-beed2d58 {
  font-size: 30rpx;
  color: #666;
  font-weight: 500;
  transition: color 0.3s ease;
}
.tab-item.active .tab-text.data-v-beed2d58 {
  color: #ff4757;
  font-weight: 600;
}
.tab-indicator.data-v-beed2d58 {
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  width: 60rpx;
  height: 6rpx;
  background: linear-gradient(135deg, #ff4757, #ff3742);
  border-radius: 3rpx;
  animation: slideIn-beed2d58 0.3s ease;
}
@keyframes slideIn-beed2d58 {
from {
    width: 0;
    opacity: 0;
}
to {
    width: 60rpx;
    opacity: 1;
}
}


